{
  "name": "angular4-webpack2-quickstart",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "concurrently 'npm run webpack-dev-server' -k -s first -n start --prefix [{name}:{pid}] -c magenta #black,red,green,yellow,blue,magenta,cyan,white,gray,bgBlack,bgRed,bgGreen,bgYellow,bgBlue,bgMagenta,bgCyan,bgWhite",
    "webpack-dev-server": "webpack-dev-server --env development",
    "precompile": "rimraf -rf ./dist",
    "compile": "webpack --progress --colors --display-error-details",
    "ghpages": "npm run compile -- --env ghpages",
    "build": "npm run compile -- --env production",
    "preserve": "npm run build",
    "serve": "lite-server --config ls-config.js",
    "lint": "tslint --config tslint.json 'src/**/*.ts' 'e2e/**/*.ts'",
    "pretest": "npm run compile",
    "test": "concurrently 'npm run protractor' 'npm run server' --kill-others --success first -p '[{name}]' -n 'E2E,HTTP' -c 'green,cyan'",
    "protractor": "protractor ./protractor.config.js",
    "preprotractor": "npm-run-all sleep webdriver-manager-update",
    "sleep": "sleep 1",
    "webdriver-manager-update": "webdriver-manager update",
    "start-server": "browser-sync start --config bs-config.js",
    "server": "npm run start-server -- --no-notify --no-open --no-online --no-inject-changes --no-reload-on-restart",
    "compose": "docker-compose --project-name=web -f ./nginx/docker-compose.yml",
    "precompose-up": "npm run build",
    "compose-up": "npm run compose -- up -d --remove-orphans",
    "compose-down": "npm run compose -- down --remove-orphans --rmi local -v",
    "docker": "npm-run-all build docker:*",
    "docker:rm": "docker rm -f daggerok-learn-jvm | true",
    "docker:rmi": "docker rmi -f daggerok/learn-jvm:latest | true",
    "docker:build": "docker build --rm -t daggerok/learn-jvm:latest -f ./nginx/Dockerfile .",
    "docker:tag": "docker tag daggerok/learn-jvm:latest daggerok/learn-jvm:$(date +%Y-%m-%d-%H)",
    "docker:push": "docker push daggerok/learn-jvm:$(date +%Y-%m-%d-%H)",
    "docker:tag-latest": "docker tag daggerok/learn-jvm:$(date +%Y-%m-%d-%H) daggerok/learn-jvm:latest",
    "docker:push-latest": "docker push daggerok/learn-jvm:latest"
  },
  "pre-push": [
    "test",
    "lint",
    "build"
  ],
  "private": true,
  "dependencies": {
    "@angular/common": ">=4.0.0-rc.5 <5.0.0",
    "@angular/compiler": ">=4.0.0-rc.5 <5.0.0",
    "@angular/core": ">=4.0.0-rc.5 <5.0.0",
    "@angular/forms": ">=4.0.0-rc.5 <5.0.0",
    "@angular/http": ">=4.0.0-rc.5 <5.0.0",
    "@angular/platform-browser": ">=4.0.0-rc.5 <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0-rc.5 <5.0.0",
    "@angular/router": ">=4.0.0-rc.5 <5.0.0",
    "angular2-highlight-js": "^4.0.0",
    "core-js": "2.4.1",
    "normalize.css": "6.0.0",
    "rxjs": "5.2.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": ">=4.0.0-rc.5 <5.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "7.0.11",
    "@types/selenium-webdriver": "3.0.0",
    "@ultimate/aot-loader": "0.1.15",
    "angular2-router-loader": "0.3.5",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "6.7.7",
    "awesome-typescript-loader": "3.1.2",
    "babel-loader": "6.4.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.2.2",
    "babel-preset-stage-0": "6.22.0",
    "base-href-webpack-plugin": "1.0.2",
    "browser-sync": "2.18.8",
    "codelyzer": "<3.0.0",
    "compression-webpack-plugin": "0.3.2",
    "concurrently": "3.4.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "^3.2.4",
    "css-loader": "0.27.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "image-webpack-loader": "3.3.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "less": "2.7.2",
    "less-loader": "4.0.2",
    "lite-server": "2.3.0",
    "node-sass": "4.5.1",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "pre-push": "0.1.1",
    "precss": "1.4.0",
    "protractor": "5.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "rucksack-css": "0.9.1",
    "sass-loader": "6.0.3",
    "script-ext-html-webpack-plugin": "1.7.1",
    "source-map-loader": "0.2.0",
    "style-loader": "0.16.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "ts-node": "3.0.2",
    "tslint": "^4.5.1",
    "typescript": "2.2.2",
    "url-loader": "0.5.8",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 4 versions",
              "safari >= 7"
            ]
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "syntax-dynamic-import",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "description": "angular4 using @angular/cli and webpack 2",
  "main": "webpack.config.js",
  "keywords": [
    "angular 4",
    "@angular/cli",
    "webpack 2"
  ],
  "author": "daggerok",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angular4.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/angular4/issues"
  },
  "homepage": "https://github.com/daggerok/angular4#readme"
}
